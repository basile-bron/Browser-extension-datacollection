{
  "manifest_version": 2,

  "name": "Chrome Extension Skeleton",
  "description": "Chrome Extension Skeleton",
  "version": "1.0",
  "content_scripts": [
    {
  	"matches":  ["https://www.youtube.com/", "https://www.youtube.com/watch*"],
  	"js": ["jquery.js","content.js"],
  	"run_at": "document_end"
    }
  ],
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
